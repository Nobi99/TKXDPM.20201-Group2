{
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      ["@babel/plugin-transform-template-literals", {
        "loose": true
      }],
      ["@babel/plugin-transform-async-to-generator", {
        "module": "bluebird",
        "method": "coroutine"
      }]
    ]
    }